pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--pyoro
--@z6v / cmrn.io

function _init()
	startmenu()
end

function _draw()
	cls()
	if(gs.draw)gs.draw()
end

function _update60()
	if(gs.update)gs.update()
end

--==============================
-- game state start
--==============================

function startgame()
	gs={}
	gs.draw=drawgame
	gs.update=updategame
	
	settrans(3)
	
	actors={}
	makeplayer()
	makefloor()
end

function makeplayer()
	local p={x=56,y=105}
	p.wheight=0
	p.tongue=nil
	p.footsprite=4
	p.f=false
	p.ms=1
	p.t=0
	p.sprloc={x=8,y=0}
	p.draw=function()
		local footpos=5
		if(p.f)footpos=4
		sspr(40,p.footsprite,7,3,p.x+footpos,p.y+12,7,3,p.f)
		sspr(p.sprloc.x,p.sprloc.y,16,14,p.x,p.y+p.wheight,16,14,p.f)
	end
	
	p.update=function()
		local m=false
		-- walkywalk
		if btn(0) then
			p.x-=p.ms
			p.f=false
			m=true
		end
		if btn(1) then
			p.x+=p.ms
			p.f=true
			m=true
		end
		-- walky timer
		if m then
			p.t+=1
		else
			p.t=0
			p.wheight=0
		end
		
		if p.t > 2 then
			p.t=0
			if p.wheight==0 then
				p.wheight=1
			else
				p.wheight=0
			end
		end
		
		if btn(5) then
			p.wheight=-1
			p.sprloc.x=24
		else
			p.sprloc.x=8
		end
		
		if(p.x<0)p.x=0
		if(p.x>128-16)p.x=128-16
	end
	
	add(actors,p)
	player=p
end

function makefloor()
	floor={}
	
	local l=0
	local fwidth=8
	while l<128 do
		local f={x=l,y=120,a=true,s=21}
		l+=fwidth
		
		add(floor,f)
		add(actors,f)
	end
end

function updategame()
	for a in all(actors) do
		if a.update then
			a.update()
		elseif a.dx or a.dy then
			a.x+=a.dx
			a.y+=a.dy
		end
	end
end

function drawgame()
	drawbackground()
	
	for a in all(actors) do
		if a.draw then
			a.draw()
		elseif a.s then
			spr(a.s,a.x,a.y)
		end
	end
end

function drawbackground()
	cls(12)
end

--//////////////////////////////
-- game state end
--//////////////////////////////

--==============================
-- menu state start
--==============================

function startmenu()
	gs={}
	gs.update=updatemenu
	gs.draw=drawmenu
end

function updatemenu()
	if(btnp(5))startgame()
end

function drawmenu()
	print("pyoro - press — to start", 20, 20, 7)
end

--//////////////////////////////
-- menu state end
--//////////////////////////////

-- useful functions

--sets all pal colours to c
--or resets pal if c isn't given
function setpal(c)
	for i=0,16 do
		pal(i,c or i)
	end
end

local ctrans=0
function settrans(c)
	c=c or 0
	palt(ctrans,false)
	palt(c,true)
	ctrans=c
end

--bordered sprite
--draws a sprite with a 1px border
-- of colour b
function bspr(n,x,y,w,h,f,b)
	w=w or 1
	h=h or 1
	f=f or false
	b=b or 0
	
	-- border
	setpal(b)
	for i=-1,1,2 do
		spr(n,x+i,y,w,h,f)
		spr(n,x,y+i,w,h,f)
	end
	setpal()
	
	-- normal sprite
	spr(n,x,y,w,h,f)
end
